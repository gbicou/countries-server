{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "generate": {
      "cache": false
    },
    "build": {
      "dependsOn": ["generate", "^build"],
      "outputs": [".output/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["generate"]
    },
    "stub": {
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["generate", "stub"],
      "cache": false,
      "persistent": true
    }
  }
}
