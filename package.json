{
  "name": "@bicou/countries-server",
  "version": "1.3.3",
  "description": "graphql sample server",
  "homepage": "https://github.com/gbicou/countries-server",
  "keywords": [
    "graphql",
    "typescript",
    "pothos",
    "graphql-yoga",
    "vercel",
    "nitropack"
  ],
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/countries-server",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "nitro prepare",
    "dev": "nitro dev",
    "build": "nitro build",
    "unbuild": "unbuild",
    "start": "node .output/server/index.mjs",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "semantic-release",
    "bootstrap": "prisma migrate deploy && prisma db seed"
  },
  "dependencies": {
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-relay": "^3.43.0",
    "@prisma/client": "4.15.0",
    "graphql-yoga": "^4.0.0"
  },
  "devDependencies": {
    "@bicou/semantic-release": "^1.0.1",
    "@pothos/plugin-prisma": "^3.52.0",
    "@tsconfig/node20": "^1.0.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^47.0.0",
    "graphql": "^16.6.0",
    "h3": "^1.6.6",
    "nitropack": "^2.4.1",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "semantic-release": "^21.0.5",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "~5.0.4",
    "unbuild": "^1.2.1"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.6.2"
}
