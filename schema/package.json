{
  "name": "@bicou/countries-server-schema",
  "version": "1.4.1",
  "description": "graphql countries server schema",
  "homepage": "https://github.com/gbicou/countries-server",
  "keywords": [
    "graphql",
    "typescript",
    "pothos"
  ],
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/countries-server",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    }
  },
  "main": "./dist/schema.cjs",
  "types": "./dist/schema.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "tsup": {
    "entry": [
      "src/schema.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "clean": true
  },
  "scripts": {
    "dev": "tsup --watch",
    "stub": "tsup --dts-only",
    "build": "tsup --minify"
  },
  "dependencies": {
    "@bicou/countries-server-data": "workspace:*",
    "@pothos/core": "^3.41.1"
  },
  "devDependencies": {
    "graphql": "^16.8.1"
  },
  "peerDependencies": {
    "graphql": "^16.8.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "name": "schema"
  }
}
