{
  "name": "@bicou/countries-server-schema",
  "version": "1.7.0",
  "description": "graphql countries server schema",
  "homepage": "https://github.com/gbicou/countries-server",
  "keywords": [
    "graphql",
    "typescript",
    "pothos"
  ],
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "license": "MIT",
  "repository": "github:gbicou/countries-server",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    }
  },
  "main": "./dist/schema.cjs",
  "types": "./dist/schema.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "tsup": {
    "entry": [
      "src/schema.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "clean": true
  },
  "scripts": {
    "dev": "tsup --watch",
    "stub": "tsup --dts-only",
    "build": "tsup --minify",
    "lint": "eslint ."
  },
  "dependencies": {
    "@bicou/countries-server-data": "workspace:*",
    "@pothos/core": "^4.3.0"
  },
  "devDependencies": {
    "@private/eslint-config": "workspace:*",
    "eslint": "9.17.0",
    "graphql": "16.10.0",
    "tsup": "8.3.5"
  },
  "peerDependencies": {
    "graphql": "^16.10.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
